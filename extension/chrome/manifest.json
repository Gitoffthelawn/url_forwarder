{
    "name": "URL Forwarder",
    
    "description": "Redirect, block and do native processing of links.",
    
    "version": "1.0",
  
    "browser_action": {
        "browser_style": true,
        "default_popup": "popup.html"
        //"default_icon": "icon.png"
    },
    
    "background": {
        "scripts": ["background.js"]
    },
    
     "options_ui": {
        "page": "options.html",
        "chrome_style": true
     },
    
    "permissions": 
    [
        "nativeMessaging",
        "tabs",
        "background",
        "webRequest", 
        "<all_urls>",
        "webRequestBlocking",
        "<all_urls>",
        "storage"
    ],
  
    "manifest_version": 2
}
